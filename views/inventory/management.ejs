<!-- Content Heading -->
<% if (title) { %>
<h1><%= title %></h1>
<% } else { res.redirect('/') } %>

<!-- Flash Message -->
<% if (Array.isArray(messages) && messages.length > 0) { %>
<div class="flash-messages">
  <% messages.forEach(function(msg) { %>
  <p><%= msg %></p>
  <% }) %>
</div>
<% } %>

